<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="test1.nh.com.demos1.activities.collection_activity.TestCollectionActivity">


    <TextView
        android:id="@+id/hashmap_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />


    <TextView
        android:id="@+id/gouwuche"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <Button
            android:text="0+"
            android:id="@+id/add0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="1+"
            android:id="@+id/add1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="2+"
            android:id="@+id/add2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="3+"
            android:id="@+id/add3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="4+"
            android:id="@+id/add4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="5+"
            android:id="@+id/add5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <Button
            android:text="0-"
            android:id="@+id/min0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="1-"
            android:id="@+id/min1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="2-"
            android:id="@+id/min2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="3-"
            android:id="@+id/min3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="4-"
            android:id="@+id/min4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:text="5-"
            android:id="@+id/min5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>




</LinearLayout>
